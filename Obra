using System;
using System.Collections;
namespace TP_Final
{
	public class Obra//:menu
	{
		private string Tipodeobra,TEEjecucion;
		private int Progreso,codigoObra;
		private double costo;
		private string nombreDeDuenio;
		private int dniDeDuenio;
		private int Grupo;
		private ArrayList ListaObras=new ArrayList();
		
		public Obra(string nombreDeDuenio,int dniDeDuenio,string Tipodeobra, string TEEjecucion,double costo,int Grupo)
		{
			this.Tipodeobra=Tipodeobra;
			this.nombreDeDuenio=nombreDeDuenio;
			this.dniDeDuenio=dniDeDuenio;
			this.TEEjecucion=TEEjecucion;
			this.costo=costo;
			
		}
		public Obra(){
			
		}
		
		public void Mostrar_Obras(){
			foreach(Obra obra in ListaObras){
				Console.WriteLine("{0}	{1}	{2}	{3}	{4}	{5}",nombreDeDuenio,dniDeDuenio,Tipodeobra,TEEjecucion,costo,Grupo);
			}
		}
	}
}

