using System;

namespace TP_Final
{
	public class Obra//:menu
	{
		private string Tipodeobra;
		private int TEEjecucion,Progreso,codigoObra;
		private double costo;
		protected ArrayList lista_obreros;
		private string nombreDeDuenio;
		private int dniDeDuenio;
		
		public Obra(string TObra,string nombreDeDuenio,int dni,int TEstimado,int Progreso,int Codigo,double costo)
		{
			Tipodeobra=TObra;
			this.nombreDeDuenio=nombreDeDuenio
			dni=dniDeDuenio;
			TEEjecucion=TEstimado;
			this.Progreso=Progreso;
			codigoObra=Codigo;
			this.costo=costo;
		}
	}
}